      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Pr3_Server.Frames._2Body"
      mc:Ignorable="d" 
      d:DesignHeight="370" d:DesignWidth="800"
      Title="MainBody">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="50*"/>
