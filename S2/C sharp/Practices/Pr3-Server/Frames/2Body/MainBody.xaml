        <Grid Grid.Column="0" Style="{StaticResource GridPanelFile}" HorizontalAlignment="Center" VerticalAlignment="Center">
            <GroupBox Header="GroupBox">
                <Button VerticalAlignment="Center" HorizontalAlignment="Center" Height="38" Width="142"/>
                </GroupBox>
</Grid>

        <Grid Grid.Column="1" Style="{StaticResource GridPanelFile}">

        </Grid>
        
    </Grid>
</Page>
